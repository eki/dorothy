Array bytes -> 1 1 2 3 5 8;
Array words --> 1 2 6 24 120 720;
Array words2 --> 6;

[main x;
  @loadb bytes 0 -> x;
  @print_num x;
  @loadb bytes 1 -> x;
  @print_num x;
  @loadb bytes 2 -> x;
  @print_num x;
  @loadb bytes 5 -> x;
  @print_num x;

  @loadw words 0 -> x;
  @print_num x;
  @loadw words 1 -> x;
  @print_num x;
  @loadw words 3 -> x;
  @print_num x;
  @loadw words 5 -> x;
  @print_num x;

  @storeb bytes 0 27;
  @loadb bytes 0 -> x;
  @print_num x;

  @storeb bytes 3 (-3);
  @loadb bytes 3 -> x;
  @print_num x;

  @storew words 0 27;
  @loadw words 0 -> x;
  @print_num x;

  @storew words 5 (-300);
  @loadw words 5 -> x;
  @print_num x;

  @copy_table words words2 12;
  @loadw words2 0 -> x;
  @print_num x;
  @loadw words2 1 -> x;
  @print_num x;
  @loadw words2 2 -> x;
  @print_num x;
  @loadw words2 5 -> x;
  @print_num x;

  @copy_table words2 0 2;
  @loadw words2 0 -> x;
  @print_num x;
  @loadw words2 1 -> x;
  @print_num x;
];

! output:
! ---
! - 1
! - 1
! - 2
! - 8
! - 1
! - 2
! - 24
! - 720
! - 27
! - 253
! - 27
! - -300
! - 27
! - 2
! - 6
! - -300
! - 0
! - 2

